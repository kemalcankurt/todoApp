{
    "Routes": [
        {
            "UpstreamPathTemplate": "/api/user/login",
            "UpstreamHttpMethod": [
                "POST"
            ],
            "DownstreamPathTemplate": "/api/user/login",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "user-service",
                    "Port": 5001
                }
            ],
            "RateLimitOptions": {
                "EnableRateLimiting": true,
                "Period": "1m",
                "PeriodTimespan": 60,
                "Limit": 5,
                "ClientWhitelist": []
            }
        },
        {
            "UpstreamPathTemplate": "/api/user/refresh-token",
            "UpstreamHttpMethod": [
                "POST"
            ],
            "DownstreamPathTemplate": "/api/user/refresh-token",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "user-service",
                    "Port": 5001
                }
            ],
            "RateLimitOptions": {
                "EnableRateLimiting": true,
                "Period": "1m",
                "PeriodTimespan": 60,
                "Limit": 5,
                "ClientWhitelist": []
            }
        },
        {
            "UpstreamPathTemplate": "/api/user/register",
            "UpstreamHttpMethod": [
                "POST"
            ],
            "DownstreamPathTemplate": "/api/user/register",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "user-service",
                    "Port": 5001
                }
            ],
            "RateLimitOptions": {
                "EnableRateLimiting": true,
                "Period": "1m",
                "PeriodTimespan": 60,
                "Limit": 5,
                "ClientWhitelist": []
            }
        },
        {
            "UpstreamPathTemplate": "/api/user/{everything}",
            "UpstreamHttpMethod": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "DownstreamPathTemplate": "/api/user/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "user-service",
                    "Port": 5001
                }
            ],
            "AuthenticationOptions": {
                "AuthenticationProviderKey": "Bearer"
            },
            "RateLimitOptions": {
                "EnableRateLimiting": true,
                "Period": "10s",
                "PeriodTimespan": 10,
                "Limit": 5,
                "ClientWhitelist": [
                    "admin"
                ]
            }
        },
        {
            "UpstreamPathTemplate": "/api/todo/{everything}",
            "UpstreamHttpMethod": [
                "GET",
                "PUT",
                "DELETE"
            ],
            "DownstreamPathTemplate": "/api/todo/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "todo-service",
                    "Port": 5000
                }
            ],
            "AuthenticationOptions": {
                "AuthenticationProviderKey": "Bearer"
            },
            "RateLimitOptions": {
                "EnableRateLimiting": true,
                "Period": "10s",
                "PeriodTimespan": 10,
                "Limit": 5,
                "ClientWhitelist": [
                    "admin"
                ]
            }
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:8000",
        "RequestIdKey": "OcRequestId",
        "AdministrationPath": "/admin",
        "IgnoredRoutes": [
            "/healthz"
        ],
        "RateLimitOptions": {
            "ClientIdHeader": "Client-Id",
            "DisableRateLimitHeaders": false,
            "QuotaExceededMessage": "Too many requests. Please try again later.",
            "RateLimitCounterPrefix": "ocelot-ratelimit",
            "HttpStatusCode": 429
        }
    }
}